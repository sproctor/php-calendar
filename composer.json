{
  "name": "php-calendar/php-calendar",
  "description": "PHP Calendar",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Sean Proctor",
      "email": "sproctor@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "8.0.2"
    }
  },
  "replace": {
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*"
  },
  "require": {
    "php": ">=8.0",
    "laminas/laminas-config": "~3.7",
    "firebase/php-jwt": "^6.0",
    "doctrine/orm": "^2.6.2",
    "symfony/http-foundation": "6.0.*",
    "symfony/form": "6.0.*",
    "twig/twig": "^3.3",
    "twig/extra-bundle": "^3.3",
    "twig/intl-extra": "^3.3",
    "twig/markdown-extra": "^3.3",
    "symfony/security-csrf": "6.0.*",
    "symfony/templating": "6.0.*",
    "symfony/twig-bridge": "6.0.*",
    "symfony/error-handler": "6.0.*",
    "symfony/validator": "6.0.*",
    "symfony/translation": "6.0.*",
    "symfony/finder": "6.0.*",
    "ext-pdo": "*"
  },
  "suggest": {
    "ext-intl": "Allows internationalization"
  },
  "autoload": {
    "psr-4": {
      "PhpCalendar\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "laravel/homestead": "^13.0"
  }
}
