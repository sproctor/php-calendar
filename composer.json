{
    "name": "php-calendar/php-calendar",
    "description": "PHP Calendar",
    "license": "Apache 2.0",
    "authors": [
        {
            "name": "Sean Proctor",
            "email": "sproctor@gmail.com"
        }
    ],
    "config": {
        "platform": {
            "php": "5.6.0"
        }
    },
    "require": {
        "php": ">=5.6.0",
        "paragonie/random_compat": "^2.0",
        "zendframework/zend-config": "~2.6",
        "firebase/php-jwt": "~5.0",
        "symfony/http-foundation": "3.4.*",
        "symfony/form": "3.4.*",
        "twig/twig": "~1.0",
        "twig/extensions": "~1.0",
        "symfony/security-csrf": "3.4.*",
        "symfony/templating": "3.4.*",
        "symfony/twig-bridge": "3.4.*",
        "symfony/debug": "^3.4",
        "symfony/validator": "^3.4",
        "symfony/translation": "3.4.*",
        "symfony/finder": "3.4.*"
    },
    "suggest": {
        "ext-intl": "Allows internationalization"
    },
    "autoload": {
        "psr-4": {
            "PhpCalendar\\": "src/"
        },
        "files": [ "src/functions.php" ]
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    }
}
